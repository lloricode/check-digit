{
    "name": "lloricode/check-digit",
    "description": "",
    "keywords": [
        "lloricode",
        "check-digit"
    ],
    "homepage": "https://github.com/lloricode/check-digit",
    "license": "MIT",
    "authors": [
        {
            "name": "Lloric Mayuga Garcia",
            "email": "lloricode@gmail.com",
            "homepage": "https://github.com/lloricode",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.22",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Lloricode\\CheckDigit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lloricode\\CheckDigit\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format-dry-run": "vendor/bin/php-cs-fixer fix --allow-risky=yes --dry-run",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
