{
    "name": "lloricode/check-digit",
    "description": "",
    "keywords": [
        "lloricode",
        "check-digit"
    ],
    "homepage": "https://github.com/lloricode/check-digit",
    "license": "MIT",
    "authors": [
        {
            "name": "Lloric Mayuga Garcia",
            "email": "lloricode@gmail.com",
            "homepage": "https://github.com/lloricode",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "pestphp/pest": "^4.0",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "rector/rector": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Lloricode\\CheckDigit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lloricode\\CheckDigit\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest -p",
        "test-coverage": "vendor/bin/pest -p --coverage",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "format-dirty": "vendor/bin/pint --dirty",
        "format-dry-run": "vendor/bin/pint --test",
        "refactor": "vendor/bin/rector process",
        "refactor-dry-run": "vendor/bin/rector process --dry-run"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
